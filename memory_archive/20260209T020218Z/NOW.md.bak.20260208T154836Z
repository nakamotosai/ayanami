#!/bin/bash
# scripts/github_sync_notify.sh

# Load environment variables (including proxies)
if [ -f "$HOME/.openclaw/.env" ]; then
    export $(grep -v '^#' "$HOME/.openclaw/.env" | xargs)
fi

LOG_FILE="logs/c7971387-24e5-4f64-9642-d35c1c668317.log"
MAX_RETRIES=5
RETRY_DELAY=10

log() {
    echo "[$(date '+%Y-%m-%d %H:%M:%S')] $1" >> "$LOG_FILE"
}

log "Starting GitHub sync..."

# Load GITHUB_TOKEN from secrets.json
SECRETS_FILE="../credentials/secrets.json"
if [ -f "$SECRETS_FILE" ]; then
    GITHUB_TOKEN=$(jq -r .GITHUB_TOKEN "$SECRETS_FILE" 2>/dev/null)
fi

SUCCESS=false
for ((i=1; i<=MAX_RETRIES; i++)); do
    log "Attempt $i/$MAX_RETRIES..."
    
    CURL_OPTS="-s --connect-timeout 15"
    [ -n "$GITHUB_TOKEN" ] && CURL_OPTS="$CURL_OPTS -H \"Authorization: token $GITHUB_TOKEN\""
    
    # Run curl. Proxy env vars should be respected if exported above.
    eval "curl $CURL_OPTS https://api.github.com/user/repos" > /dev/null
    
    if [ $? -eq 0 ]; then
        SUCCESS=true
        break
    fi
    
    log "Warning: Attempt $i failed. Retrying in ${RETRY_DELAY}s..."
    sleep $RETRY_DELAY
done

if [ "$SUCCESS" = "false" ]; then
    log "Error: GitHub API request failed after $MAX_RETRIES attempts."
    log "Sync failed."
    exit 1
fi

log "Sync completed successfully."
exit 0
</tool_output>

<tool_code>
cat > scripts/github_sync_notify.sh << 'EOF'
#!/bin/bash
# scripts/github_sync_notify.sh

# Load environment variables (including proxies)
if [ -f "$HOME/.openclaw/.env" ]; then
    # Source safely
    set -a
    source "$HOME/.openclaw/.env"
    set +a
fi

LOG_FILE="logs/c7971387-24e5-4f64-9642-d35c1c668317.log"
MAX_RETRIES=5
RETRY_DELAY=10

log() {
    echo "[$(date '+%Y-%m-%d %H:%M:%S')] $1" >> "$LOG_FILE"
}

# Ensure log file exists
mkdir -p "$(dirname "$LOG_FILE")"

log "Starting GitHub sync..."

# Load GITHUB_TOKEN from secrets.json
# Path is relative to workspace root where cron usually runs
SECRETS_FILE="../credentials/secrets.json"
GITHUB_TOKEN=""
if [ -f "$SECRETS_FILE" ]; then
    GITHUB_TOKEN=$(jq -r .GITHUB_TOKEN "$SECRETS_FILE" 2>/dev/null)
fi

SUCCESS=false
for ((i=1; i<=MAX_RETRIES; i++)); do
    log "Attempt $i/$MAX_RETRIES..."
    
    if [ -n "$GITHUB_TOKEN" ] && [ "$GITHUB_TOKEN" != "null" ]; then
        curl -s --connect-timeout 15 -H "Authorization: token $GITHUB_TOKEN" https://api.github.com/user/repos > /dev/null
    else
        curl -s --connect-timeout 15 https://api.github.com/user/repos > /dev/null
    fi
    
    if [ $? -eq 0 ]; then
        SUCCESS=true
        break
    fi
    
    log "Warning: Attempt $i failed (network or auth). Retrying in ${RETRY_DELAY}s..."
    sleep $RETRY_DELAY
done

if [ "$SUCCESS" = "false" ]; then
    log "Error: GitHub API request failed after $MAX_RETRIES attempts."
    log "Sync failed."
    exit 1
fi

log "Sync completed successfully."
exit 0
EOF
</tool_code>
<tool_output>
</tool_output>

